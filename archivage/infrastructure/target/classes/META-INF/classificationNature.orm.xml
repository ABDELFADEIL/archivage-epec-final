<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings version="2.0"
                 xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd">

    <description>Archivage domain module</description>

    <entity
            class="org.simplon.epec.archivage.domain.classificationNature.entity.ClassificationNature"
            name="ClassificationNature" access="FIELD">
        <table name="classification_nature" />

        <attributes>
            <id name="classification_nature_id">
                <column name="classification_nature_id" length="55" />
            </id>
            <basic name="classification_nature_code">
                <column name="classification_nature_code" />
            </basic>
            <basic name="duration">
                <column name="duration" />
            </basic>

        </attributes>
    </entity>
</entity-mappings>

