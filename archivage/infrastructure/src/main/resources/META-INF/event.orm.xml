<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings version="2.0"
                 xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd">

    <description>Archivage domain module</description>

    <entity
            class="org.simplon.epec.archivage.domain.event.entity.Event"
            name="Event" access="FIELD">
        <table name="event" />

        <attributes>
            <id name="id_event">
                <column name="id_event" length="55" />
            </id>
            <basic name="event_type">
                <column name="event_type" />
            </basic>
            <basic name="event_date">
                <column name="event_date" />
            </basic>
        </attributes>
    </entity>
</entity-mappings>

